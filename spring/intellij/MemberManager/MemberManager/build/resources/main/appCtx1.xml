<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="memberDao" class="org.example.di.dao.MemberDao"/>
	<bean id="daoImpl" class="org.example.di.dao.DaoImpl"/>

	
	<bean id="memberregSvc" class="org.example.di.service.MemberRegisterService2">
		<!-- MemberRegisterService2 에는 기본 생성자가 반드시 존재-->
		<property name="memberDao">
			<!-- <ref bean="memberDao"/>-->
			<bean class="org.example.di.dao.MemberDao"/>
		</property>
	</bean>
	
	<bean id="memberPwSvc"
		  class="org.example.di.service.ChangePasswordService"
	>
		<constructor-arg ref="memberDao" />
	</bean>
	











</beans>
